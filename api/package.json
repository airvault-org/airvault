{
  "name": "airvault-api",
  "version": "0.0.1",
  "description": "Simple collaborative password manager in Node.js",
  "main": "index.js",
  "scripts": {
    "db:reset": "node db/scripts/reset.js && npm run db:migrate && npm run db:seed",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "prestart": "npm run db:migrate",
    "start": "node bin/www",
    "test": "NODE_ENV=test mocha --recursive --file test/test-helpers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jbuget/airvault.git"
  },
  "keywords": [
    "collaborative",
    "password",
    "manager",
    "node.js"
  ],
  "author": "Jérémy Buget",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/jbuget/airvault/issues"
  },
  "homepage": "https://github.com/jbuget/airvault#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.2",
    "fastify-auth": "^1.0.1",
    "fastify-cors": "^5.2.0",
    "fastify-formbody": "^5.0.0",
    "fastify-helmet": "^5.0.3",
    "fastify-plugin": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "oauth2-server": "^3.1.1",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "sinon": "^9.2.2"
  }
}
